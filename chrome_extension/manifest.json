{
	"name": "shackertags",
	"version": "1",
	"manifest_version": 2,
	"description": "Add tags to chatty users.",
	"content_scripts": [{
		"matches": ["http://www.shacknews.com/chatty*"],
		"css": ["css/shackertags.css"],
		"js": [
			"js/jquery.js",
			"js/knockout.js",			
			"js/underscore.js",
			"js/shackertags.js"]}],
	"permissions": ["storage", "tabs"],
	"web_accessible_resources": [
		"img/tag_on.png",
		"img/tag_off.png"]
}